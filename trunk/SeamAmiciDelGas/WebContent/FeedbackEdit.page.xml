<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/FeedbackList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{feedbackHome.wire}"/>

   <param name="feedbackFrom"/>
   <param name="feedbackIdfeedback" value="#{feedbackHome.feedbackIdfeedback}"/>


   <navigation from-action="#{feedbackHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Feedback.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{feedbackHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Feedback.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{feedbackHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/FeedbackList.xhtml"/>
      </rule>
   </navigation>

</page>
