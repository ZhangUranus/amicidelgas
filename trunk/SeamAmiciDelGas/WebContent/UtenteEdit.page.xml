<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/UtenteList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{utenteHome.wire}"/>

   <param name="utenteFrom"/>
   <param name="utenteCodiceFiscale" value="#{utenteHome.utenteCodiceFiscale}"/>


   <navigation from-action="#{utenteHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Utente.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{utenteHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Utente.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{utenteHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/UtenteList.xhtml"/>
      </rule>
   </navigation>

</page>
