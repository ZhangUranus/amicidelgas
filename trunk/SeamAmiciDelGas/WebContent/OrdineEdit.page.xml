<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/OrdineList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{ordineHome.wire}"/>

   <param name="ordineFrom"/>
   <param name="ordineIdordine" value="#{ordineHome.ordineIdordine}"/>


   <navigation from-action="#{ordineHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Ordine.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{ordineHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Ordine.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{ordineHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/OrdineList.xhtml"/>
      </rule>
   </navigation>

</page>
