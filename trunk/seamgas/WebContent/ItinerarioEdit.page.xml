<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/ItinerarioList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{itinerarioHome.wire}"/>

   <param name="itinerarioFrom"/>
   <param name="itinerarioIditinerario" value="#{itinerarioHome.itinerarioIditinerario}"/>
   <param name="accountFrom"/>
   <param name="accountUsername" value="#{accountHome.accountUsername}"/>



   <navigation from-action="#{itinerarioHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Itinerario.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{itinerarioHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Itinerario.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{itinerarioHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/ItinerarioList.xhtml"/>
      </rule>
   </navigation>

</page>
