<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.1.xsd"
      no-conversation-view-id="/ComuneList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{comuneHome.wire}"/>

   <param name="comuneFrom"/>
   <param name="comuneIdcomune" value="#{comuneHome.comuneIdcomune}"/>
   <param name="provincesFrom"/>
   <param name="provincesIdprovinces" value="#{provincesHome.provincesIdprovinces}"/>



   <navigation from-action="#{comuneHome.persist}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Comune.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{comuneHome.update}">
      <rule>
         <end-conversation/>
         <redirect view-id="/Comune.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{comuneHome.remove}">
      <rule>
         <end-conversation/>
         <redirect view-id="/ComuneList.xhtml"/>
      </rule>
   </navigation>

</page>
